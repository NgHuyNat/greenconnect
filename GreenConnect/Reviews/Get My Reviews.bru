meta {
  name: Get My Reviews
  type: http
  seq: 3
}

get {
  url: http://localhost:3001/api/v1/reviews/user
  body: none
  auth: bearer
}

params:query {
  page: 1
  limit: 10
}

auth:bearer {
  token: {{authToken}}
}

settings {
  encodeUrl: true
  timeout: 0
}

tests {
  test("Status code is 200", function() {
    expect(res.getStatus()).to.equal(200);
  });
  
  test("Response is array of reviews", function() {
    expect(res.getBody()).to.be.an('array');
  });
}